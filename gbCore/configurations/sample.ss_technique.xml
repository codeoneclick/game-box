<ss_technique
    class_name="ss_technique_configuration"
    base_class_name="configuration"
    is_external="1">
    <attributes>
        <attribute type="std::string" property="guid" path="/ss_technique" name="guid"/>
        <attribute type="ui32" property="frame_width" path="/ss_technique" name="frame_width"/>
        <attribute type="ui32" property="frame_height" path="/ss_technique" name="frame_height"/>
        <attribute type="ui32" property="order" path="/ss_technique" name="order"/>
        <attribute type="ui32" property="skip_frames" path="/ss_technique" name="skip_frames"/>
    </attributes>
    <relationships>
    	<relationship type="material_configuration" property="material_configuration" is_to_many="0" is_external="1" path="/ss_technique" name="material"/>
        <relationship type="attachment_configuration" property="attachments_configurations" is_to_many="1" is_external="0" path="/ss_technique/attachments" name="attachment"/>
    </relationships>
</ss_technique>